{
  "name": "@numisma/types",
  "version": "0.1.0",
  "description": "Shared type definitions for Numisma",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist/**"
  ],
  "keywords": [],
  "author": "Numisma Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.5.7",
    "eslint": "^8.48.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "zod": "^3.22.2"
  }
}
